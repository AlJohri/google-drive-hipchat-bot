<div class="row">
  <%= form_for(@resource, html: {id: "form"}, url: g_drive_resources_path) do |f| %>
  <div class="col-md-4">
      <ul class="list_user_labels">
        <li class="user_input_label">Resource Path:</li>
      </ul>
  </div>
  <div class="col-md-4">
    <ul class="list_user_values">
      <li class="user_input_value"><%= f.text_field :resource_path, :placeholder => "Resource Path" %></li>
  </div>
</div>

<div class="row">
  <div class="col-md-12">
    <%= f.submit "Submit", class: "submit" %>
  </div>
</div>
<% end %>
